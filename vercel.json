{
  "builds": [
    {
      "src": "ci-cd-lab-4-backend/core/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ci-cd-lab-4-backend/core/wsgi.py"
    }
  ]
}
